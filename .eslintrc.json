{
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "overrides": [
    {
      "files": [
        "**/*.ts"
      ]
    }
  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "no-underscore-dangle": "off",
    "max-len": [2, 120],
    "no-use-before-define": "off"
  }
}
